{
  "info": { "name": "NutriTrack - Practices Management", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json" },
  "item": [
    {
      "name": "Create Practice",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "X-User-Id", "value": "11111111-1111-1111-1111-111111111111" }
        ],
        "url": { "raw": "{{endpoint}}/api/practices", "host": ["{{endpoint}}"], "path": ["api","practices"] },
        "body": { "mode": "raw",
          "raw": "{\n  \"practice_name\": \"Beber agua\",\n  \"description\": \"8 vasos diarios\",\n  \"icon\": \"💧\",\n  \"frequency_target\": 7\n}" }
      }
    },
    {
      "name": "List Practices",
      "request": {
        "method": "GET",
        "header": [ { "key": "X-User-Id", "value": "11111111-1111-1111-1111-111111111111" } ],
        "url": { "raw": "{{endpoint}}/api/practices", "host": ["{{endpoint}}"], "path": ["api","practices"] }
      }
    },
    {
      "name": "Check Practice",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "X-User-Id", "value": "11111111-1111-1111-1111-111111111111" }
        ],
        "url": { "raw": "{{endpoint}}/api/practices/check", "host": ["{{endpoint}}"], "path": ["api","practices","check"] },
        "body": { "mode": "raw", "raw": "{\n  \"practice_id\": \"{{practiceId}}\",\n  \"date\": \"2025-09-24\"\n}" }
      }
    },
    {
      "name": "Weekly Count",
      "request": {
        "method": "GET",
        "header": [ { "key": "X-User-Id", "value": "11111111-1111-1111-1111-111111111111" } ],
        "url": { "raw": "{{endpoint}}/api/practices/weekly-count?practiceId={{practiceId}}&start=2025-09-22", "host": ["{{endpoint}}"], "path": ["api","practices","weekly-count"], "query":[{"key":"practiceId","value":"{{practiceId}}"},{"key":"start","value":"2025-09-22"}] }
      }
    }
  ],
  "variable": [
    { "key": "endpoint", "value": "http://localhost:8080" },
    { "key": "practiceId", "value": "" }
  ]
}
